<h1>Listing invent_transactions</h1>

<table>
  <tr>
    <th>Invent item</th>
    <th>Invent location</th>
    <th>Status receipt</th>
    <th>Status issue</th>
    <th>Qty</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @invent_transactions.each do |invent_transaction| %>
  <tr>
    <td><%= invent_transaction.invent_item_id %></td>
    <td><%= invent_transaction.invent_location_id %></td>
    <td><%= invent_transaction.status_receipt %></td>
    <td><%= invent_transaction.status_issue %></td>
    <td><%= invent_transaction.qty %></td>
    <td><%= link_to 'Show', invent_transaction %></td>
    <td><%= link_to 'Edit', edit_invent_transaction_path(invent_transaction) %></td>
    <td><%= link_to 'Destroy', invent_transaction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Invent transaction', new_invent_transaction_path %>
