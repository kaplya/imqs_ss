<h1>Listing invent_journal_lines</h1>

<table>
  <tr>
    <th>Invent journal</th>
    <th>Journal number</th>
    <th>Invent item</th>
    <th>Qty</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @invent_journal_lines.each do |invent_journal_line| %>
  <tr>
    <td><%= invent_journal_line.invent_journal_id %></td>
    <td><%= invent_journal_line.journal_number %></td>
    <td><%= invent_journal_line.invent_item_id %></td>
    <td><%= invent_journal_line.qty %></td>
    <td><%= link_to 'Show', invent_journal_line %></td>
    <td><%= link_to 'Edit', edit_invent_journal_line_path(invent_journal_line) %></td>
    <td><%= link_to 'Destroy', invent_journal_line, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

