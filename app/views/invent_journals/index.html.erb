<h1>Listing invent_journals</h1>

<table>
  <tr>
    <th>Number</th>
    <th>Description</th>
    <th>Type</th>
    <th>Location</th>
    <th>To location</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @invent_journals.each do |invent_journal| %>
  <tr>
    <td><%= invent_journal.number %></td>
    <td><%= invent_journal.description %></td>
    <td><%= invent_journal.type %></td>
    <td><%= invent_journal.location_id %></td>
    <td><%= invent_journal.to_location_id %></td>
    <td><%= link_to 'Show', invent_journal %></td>
    <td><%= link_to 'Edit', edit_invent_journal_path(invent_journal) %></td>
    <td><%= link_to 'Destroy', invent_journal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Invent journal', new_invent_journal_path %>
