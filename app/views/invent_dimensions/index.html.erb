<h1>Listing invent_dimensions</h1>

<table>
  <tr>
    <th>Location</th>
    <th>Batch</th>
    <th>Color</th>
    <th>Size</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @invent_dimensions.each do |invent_dimension| %>
  <tr>
    <td><%= invent_dimension.location_id %></td>
    <td><%= invent_dimension.batch_id %></td>
    <td><%= invent_dimension.color_id %></td>
    <td><%= invent_dimension.size_id %></td>
    <td><%= link_to 'Show', invent_dimension %></td>
    <td><%= link_to 'Edit', edit_invent_dimension_path(invent_dimension) %></td>
    <td><%= link_to 'Destroy', invent_dimension, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

